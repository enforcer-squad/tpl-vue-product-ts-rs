{
  "name": "tpl-vue-product-ts-rs",
  "version": "1.0.0",
  "description": "typescript vue product template with rspack",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "keywords": [
    "product",
    "typescript",
    "template",
    "rspack"
  ],
  "author": "enforcer.a06@gmail.com",
  "license": "MIT",
  "packageManager": "pnpm@9.13.0",
  "lint-staged": {
    "src/**/*.{ts,js,vue,tsx,jsx}": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "src/**/*.{css,less}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "prepare": "husky",
    "dev": "cross-env NODE_ENV=development rspack serve --config ./build/dev.mjs",
    "build": "cross-env NODE_ENV=production rspack build --config ./build/prod.mjs",
    "preview": "cross-env NODE_ENV=production node ./build/preview.mjs"
  },
  "devDependencies": {
    "@babel/preset-typescript": "7.26.0",
    "@commitlint/cli": "19.6.0",
    "@commitlint/config-conventional": "19.6.0",
    "@eslint/compat": "1.2.3",
    "@eslint/js": "9.15.0",
    "@module-federation/enhanced": "0.7.6",
    "@rsdoctor/rspack-plugin": "0.4.9",
    "@rspack/cli": "1.1.1",
    "@rspack/core": "1.1.1",
    "@rspack/plugin-react-refresh": "1.0.0",
    "@types/node": "22.10.1",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@vue/babel-plugin-jsx": "1.2.5",
    "babel-loader": "9.2.1",
    "chalk": "5.3.0",
    "cross-env": "7.0.3",
    "eslint": "9.15.0",
    "eslint-plugin-react": "7.37.2",
    "eslint-plugin-react-hooks": "5.0.0",
    "globals": "15.12.0",
    "husky": "9.1.7",
    "koa": "2.15.3",
    "koa-http-proxy-server": "1.0.2",
    "koa-mount": "4.0.0",
    "koa-static": "5.0.0",
    "less-loader": "12.2.0",
    "lint-staged": "15.2.10",
    "prettier": "3.3.3",
    "react-refresh": "0.14.2",
    "typescript": "5.7.2",
    "typescript-eslint": "8.16.0",
    "typescript-plugin-css-modules": "5.1.0",
    "typescript-vue-plugin": "1.8.27",
    "unplugin-auto-import": "19.1.0",
    "unplugin-element-plus": "0.9.1",
    "unplugin-vue-components": "28.1.0",
    "vue-loader": "17.4.2",
    "webpack-bundle-analyzer": "4.10.2",
    "webpack-merge": "6.0.1"
  },
  "dependencies": {
    "@arco-design/web-vue": "2.56.3",
    "axios": "1.7.8",
    "core-js": "3.39.0",
    "element-plus": "2.9.4",
    "pinia": "3.0.1",
    "vue": "3.5.13",
    "vue-router": "4.5.0"
  }
}
